{
  "manifest_version": 2,
  "name": "Keeper",
  "description": "Tracks who you give your data to",
  "version": "0.1",
  "author": "Ruby Childs",
  "browser_action": {
     "default_icon": "tab-icon.png",
     "default_title": "Making you own your data"
   },
  "chrome_url_overrides": {
     "newtab": "newtab.html"
  },
   "permissions": ["activeTab"],
   "content_scripts": [
     {
       "matches": [
         "<all_urls>"
       ],
       "js": ["js/jquery-2.2.4.js", "js/content.js"]
     }
   ],
   "background": {
     "scripts": ["js/background.js"]
   }
}
